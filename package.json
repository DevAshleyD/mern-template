{
  "name": "mern-template",
  "version": "1.0.0",
  "description": "A MERN template that can be used to quickly setup an application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "npx nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksentak/mern-template.git"
  },
  "author": "Keaton Sentak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ksentak/mern-template/issues"
  },
  "homepage": "https://github.com/ksentak/mern-template#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.11"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
